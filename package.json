    "uuid": "^8.3.2"
  },
  "devEngines": {
    "node": ">=15.x",
    "yarn": ">=1.x"
  },
  "browserslist": "electron 10.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
