{
  "name": "looking-glass",
  "productName": "The Looking-Glass",
  "version": "0.0.2",
  "description": "Electron application to browse various website content from one app. Built for development practice only.",
  "main": "./main.prod.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reptarsrage/looking-glass.git"
  },
  "author": {
    "name": "Reptarsrage",
    "email": "reptarsrage@gmail.com",
    "url": "https://github.com/reptarsrage"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/reptarsrage/looking-glass/issues"
  },
  "keywords": [
    "electron",
    "react",
    "redux",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/reptarsrage/looking-glass#readme",
  "scripts": {
    "build-main": "webpack -p --config ./webpack.main.config.js",
    "build-renderer": "webpack -p --config ./webpack.renderer.config.js",
    "build": "cross-env NODE_ENV=production concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "start": "cross-env ELECTRON_IS_DEV=0 NODE_ENV=production electron .",
    "start-dev": "wait-on http://localhost:4000 && electron .",
    "dev-server": "webpack-dev-server --config ./webpack.renderer.config.js",
    "dev": "cross-env NODE_ENV=development ELECTRON_IS_DEV=1 concurrently -k \"yarn dev-server\" \"yarn start-dev\"",
    "dist": "electron-builder",
    "release": "standard-version",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --format=pretty"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "pretty-quick --staged"
    ],
    "*.{css,scss}": [
      "stylelint --ignore-path .eslintignore --syntax scss --fix"
    ],
    "*.{html,md,yml}": [
      "pretty-quick --staged"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "concurrently": "5.3.0",
    "cross-env": "7.0.2",
    "csp-html-webpack-plugin": "4.0.0",
    "css-loader": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "electron": "10.1.2",
    "electron-builder": "22.8.1",
    "electron-devtools-installer": "3.1.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "eslint": "7.9.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-formatter-pretty": "4.0.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "4.4.1",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lint-staged": "10.4.0",
    "mini-css-extract-plugin": "0.11.2",
    "node-sass": "4.14.1",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "sinon": "9.0.3",
    "standard-version": "9.0.0",
    "style-loader": "1.2.1",
    "stylelint": "13.7.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "wait-on": "5.2.0",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@mixmaxhq/promise-pool": "^2.0.0",
    "axios": "^0.20.0",
    "clsx": "^1.1.1",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.4.8",
    "custom-electron-titlebar": "^3.2.4",
    "electron-dl": "^3.0.2",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.2.4",
    "electron-store": "^6.0.0",
    "electron-updater": "4.3.5",
    "framer-motion": "^2.6.15",
    "get-port": "^5.1.1",
    "history": "4.10.1",
    "image-size": "^0.9.1",
    "immer": "^7.0.9",
    "lodash": "^4.17.20",
    "memoize-one": "^5.1.1",
    "mime-types": "^2.1.25",
    "moment": "^2.28.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.1",
    "react-resize-detector": "^5.2.0",
    "react-router": "^5.2.0",
    "react-router-prop-types": "^1.0.5",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "uuid": "^8.3.0"
  },
  "devEngines": {
    "node": ">=13.x",
    "npm": ">=6.x",
    "yarn": ">=1.x"
  },
  "browserslist": "electron 9.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
