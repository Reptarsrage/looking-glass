{
  "name": "looking-glass",
  "productName": "The Looking Glass",
  "version": "0.4.25",
  "description": "Electron application to browse various website content from one app. Built for development practice only.",
  "main": "dist_main/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reptarsrage/looking-glass.git"
  },
  "author": {
    "name": "Reptarsrage",
    "email": "reptarsrage@gmail.com",
    "url": "https://github.com/reptarsrage"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/reptarsrage/looking-glass/issues"
  },
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "homepage": "https://github.com/reptarsrage/looking-glass#readme",
  "scripts": {
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "clean": "rimraf dist dist_main dist_renderer",
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "dist": "npm run build && electron-builder",
    "format": "prettier .",
    "lint": "eslint . --ext ts,tsx --cache",
    "pack": "npm run build && electron-builder --dir",
    "prepare": "husky install",
    "release": "standard-version",
    "start": "yarn dist && start \"./dist/win-unpacked/The Looking-Glass.exe\"",
    "type-check": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@mixmaxhq/promise-pool": "^2.0.0",
    "@popperjs/core": "^2.11.6",
    "@react-hook/debounce": "^4.0.0",
    "@react-hook/intersection-observer": "^3.1.1",
    "@react-hook/resize-observer": "^1.2.6",
    "@react-spring/web": "^9.6.1",
    "@use-gesture/react": "^10.2.23",
    "@vitejs/plugin-react": "^3.0.1",
    "clsx": "^1.2.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-dl": "^3.5.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "image-size": "^1.0.2",
    "memoize-one": "^6.0.0",
    "mime-types": "^2.1.35",
    "nanoid": "3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-popper-tooltip": "^4.4.2",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.6.1",
    "sharp": "^0.31.3",
    "tiny-invariant": "^1.3.1",
    "vite": "^4.0.4",
    "zustand": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite-plugin-svgr": "^2.4.0"
  }
}
