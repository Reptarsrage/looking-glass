{
  "name": "looking-glass",
  "productName": "The Looking Glass",
  "version": "0.4.25",
  "description": "Electron application to browse various website content from one app. Built for development practice only.",
  "main": "electron/electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reptarsrage/looking-glass.git"
  },
  "author": {
    "name": "Reptarsrage",
    "email": "reptarsrage@gmail.com",
    "url": "https://github.com/reptarsrage"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/reptarsrage/looking-glass/issues"
  },
  "keywords": [
    "electron",
    "react",
    "redux"
  ],
  "homepage": "https://github.com/reptarsrage/looking-glass#readme",
  "scripts": {
    "format": "prettier --write .",
    "lint": "tsc --pretty --noEmit",
    "dev:renderer": "cross-env BROWSER=none vite",
    "dev:main": "cross-env ELECTRON_IS_DEV=1 electron .",
    "dev": "concurrently -k \"yarn run dev:renderer\" \"yarn run dev:main\"",
    "build:main": "electron-builder",
    "build:renderer": "tsc && vite build",
    "build": "yarn run build:renderer && yarn run build:main",
    "dist": "yarn run build",
    "release": "standard-version",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mixmaxhq/promise-pool": "^2.0.0",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.3",
    "@react-spring/web": "^9.4.4",
    "@tanstack/react-query": "^4.2.3",
    "@use-gesture/react": "^10.2.19",
    "electron-dl": "^3.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.6",
    "electron-updater": "^5.0.1",
    "image-size": "^1.0.1",
    "memoize-one": "^6.0.0",
    "mime-types": "^2.1.35",
    "nanoid": "^3.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^2.0.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^20.1.1",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "nano-staged": "^0.8.0",
    "prettier": "^2.6.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.8",
    "typescript": "4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-svgr": "^2.1.0",
    "wait-on": "^6.0.1"
  },
  "prettier": {
    "printWidth": 120
  },
  "nano-staged": {
    "*.{js,ts,jsx,tsx,json,yml,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
