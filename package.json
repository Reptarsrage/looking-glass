{
  "name": "looking-glass",
  "productName": "The Looking-Glass",
  "version": "0.0.1",
  "description": "Electron application to browse various website content from one app. Built for development practice only.",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reptarsrage/looking-glass.git"
  },
  "author": {
    "name": "Reptarsrage",
    "email": "reptarsrage@gmail.com",
    "url": "https://github.com/reptarsrage"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/reptarsrage/looking-glass/issues"
  },
  "keywords": [
    "electron",
    "react",
    "redux",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/reptarsrage/looking-glass#readme",
  "scripts": {
    "clean": "rimraf dist release .eslintcache",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack.config.js",
    "start": "yarn build && cross-env ELECTRON_IS_DEV=0 electron .",
    "start-dev": "wait-on http://localhost:4000 && electron .",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
    "dev": "cross-env NODE_ENV=development concurrently -k \"yarn dev-server\" \"yarn start-dev\"",
    "package-ci": "yarn build && electron-builder --publish always",
    "package-linux": "yarn build && electron-builder build --linux",
    "package-win": "yarn build && electron-builder build --win --x64",
    "test": "jest",
    "test-cov": "jest --coverage",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty .",
    "lint-check": "eslint --print-config ./src/index.jsx | eslint-config-prettier-check",
    "lint-fix": "yarn --silent lint --fix",
    "lint-styles": "stylelint --ignore-path .eslintignore \"**/*.*(css|scss)\" --syntax scss",
    "lint-styles-fix": "yarn --silent lint-styles --fix"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --format=pretty"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "pretty-quick --staged"
    ],
    "*.{css,scss}": [
      "stylelint --ignore-path .eslintignore --syntax scss --fix"
    ],
    "*.{html,md,yml}": [
      "pretty-quick --staged"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "concurrently": "5.1.0",
    "cross-env": "7.0.2",
    "csp-html-webpack-plugin": "3.0.4",
    "css-loader": "3.4.2",
    "electron": "8.2.5",
    "electron-builder": "22.3.6",
    "electron-devtools-installer": "2.2.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.8",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "sass-loader": "8.0.2",
    "sinon": "8.1.1",
    "spectron": "10.0.1",
    "style-loader": "1.1.3",
    "stylelint": "13.2.1",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "wait-on": "4.0.1",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.0",
    "clsx": "^1.1.0",
    "connected-react-router": "^6.7.0",
    "core-js": "^3.4.8",
    "electron-dl": "^3.0.0",
    "electron-is-dev": "^1.1.0",
