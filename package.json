    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.2",
    "@mui/styles": "^5.0.1",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "electron-dl": "^3.0.2",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.1",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "framer-motion": "^5.0.0",
    "get-port": "6.0.0",
    "image-size": "^1.0.0",
    "immer": "^9.0.1",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.28",
    "moize": "^6.0.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.0.0",
