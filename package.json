    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-intersection-observer": "^8.25.2",
    "react-redux": "^7.1.3",
    "react-resize-detector": "^4.2.1",
    "react-router": "^5.1.2",
    "react-router-prop-types": "^1.0.4",
